#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theia/myapp/node_modules/.pnpm/prebuild-install@7.1.2/node_modules/prebuild-install/node_modules:/home/theia/myapp/node_modules/.pnpm/prebuild-install@7.1.2/node_modules:/home/theia/myapp/node_modules/.pnpm/node_modules:/home/theia/myapp/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/myapp/node_modules/.pnpm/sqlite3@5.1.7/node_modules/sqlite3/node_modules:/home/theia/myapp/node_modules/.pnpm/sqlite3@5.1.7/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/theia/myapp/node_modules/.pnpm/prebuild-install@7.1.2/node_modules/prebuild-install/node_modules:/home/theia/myapp/node_modules/.pnpm/prebuild-install@7.1.2/node_modules:/home/theia/myapp/node_modules/.pnpm/node_modules:/home/theia/myapp/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/myapp/node_modules/.pnpm/sqlite3@5.1.7/node_modules/sqlite3/node_modules:/home/theia/myapp/node_modules/.pnpm/sqlite3@5.1.7/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../prebuild-install@7.1.2/node_modules/prebuild-install/bin.js" "$@"
else
  exec node  "$basedir/../../../../../prebuild-install@7.1.2/node_modules/prebuild-install/bin.js" "$@"
fi
